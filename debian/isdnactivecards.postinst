#! /bin/sh
# postinst script for isdnactivecards

set -e

case "$1" in
    configure)
        cd /dev
        echo "Note: running MAKEDEV to create CAPI devices in /dev..."
        /sbin/MAKEDEV capi
	;;

    abort-upgrade|abort-remove|abort-deconfigure)
    	;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 0
    	;;
esac

#DEBHELPER#

exit 0
